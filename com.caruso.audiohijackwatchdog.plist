<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN"
   "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.caruso.audiohijackwatchdog</string>

    <key>ProgramArguments</key>
    <array>
        <string>/Users/caruso/scripts/audiohijack-watchdog.sh</string>
    </array>

    <key>RunAtLoad</key>
    <true/>

    <key>KeepAlive</key>
    <true/>

    <key>StandardOutPath</key>
    <string>/tmp/audiohijack-watchdog.out</string>
    <key>StandardErrorPath</key>
    <string>/tmp/audiohijack-watchdog.err</string>
</dict>
</plist>
